  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config internal/webpack/webpack.client.js --hot",
    "start-weinre": "set WEINRE=true && npm run start",
    "build": "rimraf dist && cross-env NODE_ENV=production APP_ENV=stage webpack --config internal/webpack/webpack.client.js --progress --hide-modules",
    "build-release": "rimraf dist && cross-env NODE_ENV=production APP_ENV=release webpack --config internal/webpack/webpack.client.js --progress --hide-modules",
    "build-server": "cross-env NODE_ENV=production BABEL_ENV=ssr webpack --config internal/webpack/webpack.server.js --progress --hide-modules",
    "build-ssr": "set SSR=true && npm run build && npm run build-server && node internal/script/SSR",
    "generator": "plop --plopfile ./internal/generator/index.js",
    "storybook": "start-storybook -p 9001 -c ./internal/storybook",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest",
    "find-node": "tasklist /FI \"IMAGENAME eq node.exe\"",
    "kill-node": "taskkill /F /PID",
    "lint-js": "eslint --ext .js --ext .vue .",
    "lint-staged": "lint-staged",
    "m": "gulp m",
    "lint": "stylelint app/scss/**/*.scss --syntax scss",
    "lint:fix": "stylelint app/scss/**/*.scss --syntax scss --fix",
    "stylelint": "stylelint '**/*.scss'"
  },
  "lint-staged": {
    "*.js": "lint-js",
    "*.vue": "lint-js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.0",
